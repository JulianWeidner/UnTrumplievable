<p id="notice"><%= notice %></p>

<h1>Real Tweets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_tweets.each do |real_tweet| %>
      <tr>
        <td><%= link_to 'Show', real_tweet %></td>
        <td><%= link_to 'Edit', edit_real_tweet_path(real_tweet) %></td>
        <td><%= link_to 'Destroy', real_tweet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real Tweet', new_real_tweet_path %>
