<div id="nav">
  <% if current_user %>
    <%= link_to "Edit Profile", edit_faker_path(current_user.id) %>
    <%= link_to "Logout", :logout, method: :post %>
  <% else %>
    <%= link_to "Register", new_faker_path %> |
    <%= link_to "Login", :login %>
  <% end %>
</div>

<div>
  <p id="notice"><%= flash[:notice] %></p>
  <p id="alert"><%= flash[:alert] %></p>
</div>